msgid "Unknown version number"
msgstr "Unbekannte Versionsnummer"

#: prefs.js
msgid "Main Settings"
msgstr "Grundeinstellungen"

msgid "<b>Behavior</b>"
msgstr "<b>Verhalten</b>"

msgid "Dock is fixed and always visible"
msgstr "Die Dock hat eine feste Position und ist immer sichtbar"

msgid "Animation time [ms]"
msgstr "Animationsdauer [ms]"

msgid "Show delay [ms]"
msgstr "Einblendeverzögerung [ms]"

msgid "Hide delay [ms]"
msgstr "Ausblendeverzögerung [ms]"

msgid "Hover+click to show dock when window maximized"
msgstr "Mit der Maus drüberfahren und klicken, um Dock anzuzeigen, wenn Fenster maximiert ist"

msgid "Leave dock edge visible when slid out"
msgstr "Dock-Kante sichtbar lassen beim Ausblenden"

msgid "Autohide"
msgstr "Autoverstecken"

msgid "Intellihide"
msgstr "Intelligentes Verstecken"

msgid "Dodge all windows"
msgstr "Allen Fenstern ausweichen"

msgid "Dodge all instances of focused app"
msgstr "Allen Fenstern des fokusierten Programms ausweichen"

msgid "Dodge only top instance of focused app"
msgstr "Nur dem aktuell fokusiertem Fenster ausweichen"

msgid "<b>Background</b>"
msgstr "<b>Hintergrund</b>"

msgid "Customize the dock background opacity"
msgstr "Dock-Hintergrundtransparenz anpassen"

msgid "Opacity"
msgstr "Transparenz"

msgid "Only when in autohide"
msgstr "Nur im Autoverstecken-Modus"

msgid "<b>Position</b>"
msgstr "<b>Position</b>"

msgid "Show the dock on following monitor (if attached)"
msgstr "Zeige Dock auf folgendem Monitor (falls angeschlossen)"

msgid "Primary (default)"
msgstr "Hauptmonitor (Vorgabe)"

msgid "1"
msgstr "1"

msgid "2"
msgstr "2"

msgid "3"
msgstr "3"

msgid "4"
msgstr "4"

msgid "Extend the height of the dock to fill the screen"
msgstr "Die Höhe auf die Bildschirmhöhe ausdehnen"

msgid "Top Margin"
msgstr "Oberer Rand"

msgid "Bottom Margin"
msgstr "Unterer Rand"

msgid "Additional Settings"
msgstr "Weitere Einstellungen"

msgid "<b>Workspace Captions</b>"
msgstr "<b>Arbeitsflächenbeschriftungen</b>"

msgid "User theme supports workspaces-to-dock captions"
msgstr "Nutzerthema unterstützt workspaces-to-dock-Beschriftungen"

msgid "Add captions to workspace thumbnails"
msgstr "Beschriftung auf Arbeitsflächenvorschau anzeigen"

msgid "Show the workspace number"
msgstr "Arbeitsflächennummer"

msgid "Show the workspace name"
msgstr "Arbeitsflächennamen"

msgid "Show the workspace window count"
msgstr "Fensteranzahl"

msgid "Show the workspace taskbar (app icons)"
msgstr "Zeige Arbeitsflächen-Task-Leiste (Programmsymbole)"

msgid "Show a spacer/filler"
msgstr "Platzhalter"

msgid "Use image"
msgstr "Nutze Bild"

msgid "Large icons"
msgstr "Große Symbole"

msgid "Expand"
msgstr "Ausdehnen"

msgid "<b>Custom Actions</b>"
msgstr "Aktionen anpassen"

msgid "Toggle overview mode with right click"
msgstr "Mit Rechtsklick zur Übersicht wechseln"

msgid "<b>Dash Integration</b>"
msgstr "<b>Dash-Integration</b>"

msgid "Show workspaces when hovering over Dash-To-Dock extension"
msgstr "Zeige Arbeitsflächen, wenn sich die Maus über der Dash-To-Dock-Erweiterung befindet"

msgid "Close all applications"
msgstr "Alle Anwendungen schließen"

msgid "Extension preferences"
msgstr "Erweiterungseinstellungen"
