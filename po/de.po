#: prefs.js:47
msgid "Behavior"
msgstr "Verhalten"

#: prefs.js:56
msgid "<b>Visibility</b>"
msgstr "<b>Sichtbarkeit</b>"

#: prefs.js:67
msgid "Dock is fixed and always visible"
msgstr "Die Dock hat eine feste Position und ist immer sichtbar"

#: prefs.js:105
msgid "Animation time [ms]"
msgstr "Animationsdauer [ms]"

#: prefs.js:126
msgid "Show delay [ms]"
msgstr "Einblendeverzögerung [ms]"

#: prefs.js:145
msgid "Hide delay [ms]"
msgstr "Ausblendeverzögerung [ms]"

#: prefs.js:164
msgid "Leave the dock edge visible when slid out"
msgstr "Dock-Kante sichtbar lassen beim Ausblenden"

#: prefs.js:174
msgid "Disable scroll to prevent workspace switching when slid out"
msgstr ""
"Scrollen deaktivieren, um versehentliches Wechseln der Arbeitsfläche zu "
"verhindern"

#: prefs.js:203
msgid "<b>Autohide</b> : Show the dock on mouse hover"
msgstr "<b>Autoverstecken</b>: Dock bei Mausberührung anzeigen"

#: prefs.js:220
msgid "Require click to show the dock when window maximized"
msgstr ""
"Mit der Maus drüberfahren und klicken, um Dock anzuzeigen, wenn Fenster "
"maximiert ist"

#: prefs.js:230
msgid "Require pressure to show the dock"
msgstr "Dock nur anzeigen bei Druck durch die Maus "

#: prefs.js:240
msgid "Pressure threshold [px]"
msgstr "Druckschwelle [px]"

#: prefs.js:288
msgid "<b>Intellihide</b> : Show the dock unless a window overlaps"
msgstr ""
"<b>Intelligentes Verstecken</b>: Dock anzeigen, solange kein Fenster im Weg "
"ist"

#: prefs.js:305
msgid "Dodge all windows"
msgstr "Allen Fenstern ausweichen"

#: prefs.js:313
msgid "Dodge all instances of focused app"
msgstr "Allen Fenstern des fokusierten Programms ausweichen"

#: prefs.js:322
msgid "Dodge only top instance of focused app"
msgstr "Nur dem aktuell fokusiertem Fenster ausweichen"

#: prefs.js:346
msgid "Ignore top panel menus"
msgstr "Menüs der oberen Leiste ignorieren"

#: prefs.js:356
msgid "Ignore message tray menus + shift dock upward when tray is shown"
msgstr ""
"Menüs der Benachrichtigungsleiste ignorieren, aber Dock bei sichtbarer "
"Leiste nach oben schieben"

#: prefs.js:393
msgid "<b>Custom Actions</b>"
msgstr "<b>Aktionen anpassen</b>"

#: prefs.js:403
msgid "Toggle Gnome Shell's overview mode with right click"
msgstr "Mit Rechtsklick zur Übersicht wechseln"

#: prefs.js:430
msgid "Toggle the dock with a keyboard shortcut"
msgstr "Das Dock mit einem Tastenkürzel öffnen"

#: prefs.js:453
msgid "Invalid accelerator. Try F12, <Super>space, <Ctrl><Alt><Shift>w, etc."
msgstr ""
"Ungültige Unterstützungstaste. Möglich sind F12, <Super>Leertaste, "
"<Strg><Alt><Shift>w, etc."

#: prefs.js:505
msgid "Appearance"
msgstr "Erscheinungsbild"

#: prefs.js:514
msgid "<b>Background</b>"
msgstr "<b>Hintergrund</b>"

#: prefs.js:525
msgid "Customize the dock background opacity"
msgstr "Dock-Hintergrundtransparenz anpassen"

#: prefs.js:539
msgid "Opacity"
msgstr "Transparenz"

#: prefs.js:560
msgid "Only when the dock is shown by autohide"
msgstr "Nur im Autoverstecken-Modus"

#: prefs.js:592
msgid "<b>Position</b>"
msgstr "<b>Position</b>"

#: prefs.js:602
msgid "Show the dock on the following monitor (if attached)"
msgstr "Zeige Dock auf folgendem Monitor (falls angeschlossen)"

#: prefs.js:604
msgid "Primary (default)"
msgstr "Hauptmonitor (Vorgabe)"

#: prefs.js:605
msgid "1"
msgstr "1"

#: prefs.js:606
msgid "2"
msgstr "2"

#: prefs.js:607
msgid "3"
msgstr "3"

#: prefs.js:608
msgid "4"
msgstr "4"

#: prefs.js:636
msgid "<b>Height</b>"
msgstr "<b>Höhe</b>"

#: prefs.js:647
msgid "Extend the height of the dock to fill the screen"
msgstr "Die Höhe auf die Bildschirmhöhe ausdehnen"

#: prefs.js:661
msgid "Top margin"
msgstr "Oberer Rand"

#: prefs.js:684
msgid "Bottom margin"
msgstr "Unterer Rand"

#: prefs.js:733
msgid "<b>Thumbnails</b>"
msgstr "<b>Vorschaubilder</b>"

#: prefs.js:744
msgid "Customize the workspace thumbnail size"
msgstr "Die Bildgröße der Arbeitsflächenvorschau festlegen"

#: prefs.js:758
msgid "Thumbnail size"
msgstr "Vorschaubildgröße"

#: prefs.js:829
msgid "Additional"
msgstr "Weitere"

#: prefs.js:838
msgid "<b>Workspace Captions</b>"
msgstr "<b>Arbeitsflächenbeschriftungen</b>"

#: prefs.js:858
msgid "Add captions to workspace thumbnails"
msgstr "Beschriftung auf Arbeitsflächenvorschau anzeigen"

#: prefs.js:874
msgid "Caption height [px]"
msgstr "Höhe der Beschriftung [px]"

#: prefs.js:895
msgid "Taskbar icon size [px]"
msgstr "Größe der Programmsymbole [px]"

#: prefs.js:917
msgid "Caption menu tasklist icon size [px]"
msgstr "Symbolgröße der Menübeschriftungen der Anwendungenliste [px]"

#: prefs.js:939
msgid "Show workspace number"
msgstr "Arbeitsflächennummer"

#: prefs.js:951 prefs.js:992 prefs.js:1042 prefs.js:1083 prefs.js:1124
msgid "Expand"
msgstr "Ausdehnen"

#: prefs.js:979
msgid "Show workspace name"
msgstr "Arbeitsflächennamen"

#: prefs.js:1019
msgid "Show workspace window count"
msgstr "Fensteranzahl"

#: prefs.js:1033
msgid "Use image"
msgstr "Nutze Bild"

#: prefs.js:1070
msgid "Show workspace taskbar (apps)"
msgstr "Zeige Arbeitsflächen-Task-Leiste (Programmsymbole)"

#: prefs.js:1110
msgid "Show a spacer/filler"
msgstr "Platzhalter"

#: prefs.js:1246
msgid "<b>Dash Integration</b>"
msgstr "<b>Dash-Integration</b>"

#: prefs.js:1256
msgid "Show the dock when hovering over Dash-To-Dock extension"
msgstr ""
"Zeige Arbeitsflächen, wenn sich die Maus über der Dash-To-Dock-Erweiterung "
"befindet"

#: prefs.js:1282
msgid "Show a favorites shortcut panel (experimental)"
msgstr "Favoritenleiste anzeigen (experimentell)"

#: prefs.js:1296
msgid "Shortcuts panel icon size [px]"
msgstr "Symbolgröße der Favoritenleiste [px]"

#: prefs.js:1317
msgid "Show running applications"
msgstr "Laufende Anwendungen anzeigen"

#: prefs.js:1327
msgid "Show places"
msgstr "Orte anzeigen"

#: prefs.js:1337
msgid "Show the applications button at the bottom"
msgstr "Anwendungenknopf unten anzeigen"

#: prefs.js:1347
msgid "Show the shortcut button popup menu arrow at the top"
msgstr "Favoritenknopf-Aufklappmenü-Pfeil oben anzeigen"

#: prefs.js:1357
msgid "Hide thumbnails when the shortcut button popup menu is shown"
msgstr ""
"Vorschaubilder ausblenden, wenn das Favoritenknopf-Aufklappmenü angezeigt "
"wird"

